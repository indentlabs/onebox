{{#image}}
  <a href="{{link}}" target="_blank" rel="noopener">
    <img src="{{image}}" class="thumbnail"/>
  </a>
{{/image}}

<h3>
  <a href="{{link}}" target="_blank" rel="noopener">{{title}}</a>
</h3>

{{#description}}
  <p>
    <span class="label1">
      {{description}}
    </span>
  </p>
{{/description}}

{{#path}}
  <p>
    <sub>
      <a href="{{link}}" target="_blank" rel="noopener">{{path}}</a>
    </sub>
  </p>
{{/path}}